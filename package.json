{
    "name": "unirwd",
    "private": true,
    "packageManager": "pnpm@10.18.1",
    "version": "1.0.0",
    "workspaces": ["apps/*", "packages/*", "services/*"],
    "scripts": {
      "dev": "turbo run dev --parallel",
      "build": "turbo run build",
      "start": "turbo run start",
      "lint": "turbo run lint",
      "format": "prettier --write .",
      "test": "turbo run test",
      "typecheck": "turbo run typecheck"
    },
    "devDependencies": {
      "typescript": "^5.4.0",
      "ts-node": "^10.9.2",
      "@types/node": "^20.11.30",
      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-import": "^2.29.0",
      "prettier": "^3.1.0",
      "turbo": "^2.0.0",
      "zx": "^8.0.0",
      "dotenv": "^16.4.0",
      "cross-env": "^7.0.3",
      "rimraf": "^5.0.0",
      "husky": "^9.0.10",
      "lint-staged": "^15.2.0",
      "commitizen": "^4.3.0",
      "cz-conventional-changelog": "^3.3.0",
      "@types/jest": "^29.5.11",
      "jest": "^29.7.0",
      "ts-jest": "^29.1.1",
      "@playwright/test": "^1.42.0"
    },
    "config": {
      "commitizen": {
        "path": "./node_modules/cz-conventional-changelog"
      }
    }
  }
  